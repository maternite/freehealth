<MedForm name="Maternity::Vaccination" uifilename="./ui/xx/vaccination.ui">
  <label lang="xx">Vaccination</label>
  <label lang="fr">Vaccination</label>
  <options>UseFormAsNameSpace</options>
  <icon>__theme__/stethoscope.png</icon>

  <Item type="group" name="Vaccination::Tuberculosis">
    <uilink widget="tuberculosisGroupBox"/>
    <label lang="xx">Tuberculosis vaccines</label>
    <label lang="fr">Vaccination antituberculeuse</label>
    <options>collapsible;expanded</options>

  <Item type="helptext" name="Mantoux::test">
    <uilink widget="mantouxLabel" />
    <label lang="xx">Mantoux test</label>
    <label lang="fr">Intradermoréaction prévaccinale à la tuberculine</label>
    <options></options>
  </Item>

  <Item type="date" name="Mantoux::injection::date">
    <uilink label="mantouxInjectionDateLabel" widget="mantouxInjectionDateEdit" />
    <label lang="xx">Injection date</label>
    <label lang="fr">Date d'injection</label>
    <options></options>
  </Item>

  <Item type="shorttext" name="Mantoux::Lot::number">
    <uilink label="mantouxLotLabel" widget="mantouxLotLineEdit" />
    <label lang="xx">Lot number</label>
    <label lang="fr">Numéro de lot</label>
    <options></options>
  </Item>

  <Item type="date" name="Mantoux::reading::date">
    <uilink label="mantouxReadingDateLabel" widget="mantouxReadingDateEdit" />
    <label lang="xx">Reading date</label>
    <label lang="fr">Date de lecture</label>
    <options></options>
  </Item>

  <Item type="spin" name="Mantoux::result">
    <uilink label="mantouxResultLabel" widget="mantouxResultSpinBox" />
    <label lang="xx">Result (diameter of induration)</label>
    <label lang="fr">Résultat (en mm d'induration)</label>
    <options></options>
  </Item>

  <Item type="shorttext" name="Mantoux::identity">
    <uilink label="mantouxAdministeredLabel" widget="mantouxAdministeredLineEdit" />
    <label lang="xx">Administered and read by</label>
    <label lang="fr">Réalisé et lu par</label>
    <placeholder lang="xx">Name of healthcare provider and/or facility</placeholder>
      <placeholder lang="fr">Nom du soignant et/ou de l'institution</placeholder>
    <options></options>
  </Item>

  <Item type="helptext" name="Tuberculosis::vaccine::label">
    <uilink widget="tbVaccinationLabel" />
    <label lang="xx">Vaccine</label>
    <label lang="fr">Vaccin</label>
    <options></options>
  </Item>

  <Item type="date" name="Tuberculosis::Vaccine::injection::date">
    <uilink label="tbInjectionDateLabel" widget="tbInjectionDateEdit" />
    <label lang="xx">Injection date</label>
    <label lang="fr">Date d'injection</label>
    <options></options>
  </Item>

  <Item type="combo" name="Tuberculosis::Vaccine::type">
    <uilink label="tbTypeLabel" widget="tbTypeComboBox" />
    <label lang="xx">Vaccine type</label>
    <label lang="fr">Type de vaccin</label>
    <value>
      <uuid id="0">BCG</uuid>
      <uuid id="1">not done</uuid>
      <!--<sctid id="3">385660001</sctid>-->
      <possible id="0" lang="xx">BCG</possible>
      <possible id="1" lang="xx">ø</possible>
      <possible id="0" lang="fr">BCG</possible>
      <possible id="1" lang="fr">ø</possible>
    </value>
    <options></options>
  </Item>

  <Item type="shorttext" name="Tuberculosis::Vaccine::Name">
    <uilink label="tbVaccineNameLabel" widget="tbVaccineNameLineEdit" />
    <label lang="xx">Vaccine name</label>
    <label lang="fr">Nom du vaccin</label>
    <options></options>
  </Item>

  <Item type="shorttext" name="Tuberculosis::Vaccine::Lot">
    <uilink label="tbLotLabel" widget="tbLotLineEdit" />
    <label lang="xx">Lot number</label>
    <label lang="fr">Numéro de lot</label>
    <options></options>
  </Item>

  <Item type="shorttext" name="Tuberculosis::Vaccine::identity">
    <uilink label="tbAdministeredLabel" widget="tbAdministeredLineEdit" />
    <label lang="xx">Administered by</label>
    <label lang="fr">Réalisé par</label>
    <placeholder lang="xx">Name of healthcare provider and/or facility</placeholder>
    <placeholder lang="fr">Nom du soignant et/ou de l'institution</placeholder>
  </Item>

  </Item> <!-- end of group tb -->


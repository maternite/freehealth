  <file type="script" name="onload">./scripts/xx/prenatal.js</file>

  <MedForm name="Maternity::Obstetrics::Prenatal" uifilename="./ui/xx/prenatalmain.ui">
    <label lang="xx">Prenatal consultation</label>                       
    <label lang="fr">Consultation prénatale</label>               
    <options>UseFormAsNameSpace</options>                                       
    <icon>__theme__/stethoscope.png</icon>                                      
<!--
    <file type="htmlprintmask">./html/xx/padtools_mask.html</file>              
    <file type="htmlexportmask">./html/xx/padtools_mask.html</file>
-->
    <Item type="group" name="Fetus">

      <uilink label="" widget="fetusGroupBox" layout="" />
      <label lang="xx">Fetus</label>                         
      <label lang="fr">Fœtus</label>                       
      <options>SummaryFontBold;expanded</options>

        <Item type="spin" name="FetusNumber">
          <uilink label="fetusNumberLabel" widget="fetusNumberSpinBox" layout="" />
          <label lang="xx">Number of fetus</label>
          <label lang="fr">Nombre de fœtus</label>
          <min>0</min>
          <max>15</max>
          <step>1</step>
        </Item>
    
    </Item>

      <Item type="group" name="Fetus1GroupBox">
        <uilink widget="fetus1GroupBox"/>
        <label lang="xx">Fetus 1</label>                       
        <label lang="fr">Fœtus 1</label>                       
        <options>DontPrintEmptyValues</options>
        
          <Item type="radio" name="Fetus1::PrenatalHeartbeat::Radio">                        
            <uilink label="prenatalHeartbeatF1Label" widget="" layout="prenatalHeartbeatF1RadioLayout"/>         
              <label lang="xx">Prenatal heartbeat</label>                            
              <label lang="fr">Bruit du cœur fœtal</label>                     
              <options>compact</options>                                      
              <value>                                                         
                <uuid id="0">yes</uuid>                                     
                <uuid id="1">no</uuid>
                <uuid id="2">∅</uuid>                                      
                <possible id="0" lang="xx">Yes</possible>                   
                <possible id="1" lang="xx">No</possible>
                <possible id="2" lang="xx">∅</possible>                    
                <print id="0" lang="xx">Prenatal heartbeat present</print>  
                <print id="1" lang="xx">Prenatal heartbeat absent</print>
                <print id="2" lang="xx">Not evaluated</print>
                <possible id="0" lang="fr">Oui</possible>                   
                <possible id="1" lang="fr">Non</possible>
                <possible id="2" lang="fr">∅</possible>                   
                <print id="0" lang="fr">Oui</print>          
                <print id="1" lang="fr">Non</print>
                <print id="2" lang="fr">∅</print>   
              </value>                                                        
          </Item>

          <Item name="Fetus1::PrenatalHeartbeat::Comment" type="shorttext">
            <uilink label="" widget="prenatalHeartbeatF1LineEdit" layout="" /> 
            <label lang="xx"></label>
            <label lang="fr"></label>
            <placeholder lang="xx">Comment</placeholder>
            <placeholder lang="fr">Commentaire</placeholder>
          </Item>          

          <Item type="uniquelist" name="F1Presentation">                   
            <uilink label="presentationLabel" widget="presentationListView" layout=""/>         
            <label lang="xx">Presentation</label>                             
            <label lang="fr">Présentation</label>                            
            <value>                                                         
            <uuid id="0">Vertex presentation</uuid> 
            <!--<sctid id="0">70028003</sctid>-->                             
            <uuid id="1">Frank breech presentation</uuid>
            <!--<sctid id="1">18559007</sctid>-->                              
            <uuid id="2">Complete breech presentation</uuid>
            <!--<sctid id="2">49168004</sctid>-->
            <uuid id="3">Incomplete breech presentation</uuid>
            <!--<sctid id="3">38049006</sctid>-->                                      
            <uuid id="4">Acromion presentation</uuid>
            <!--<sctid id="4">23954006</sctid>-->                                  
            <uuid id="5">ø</uuid>                                
            <possible id="0">Vertex</possible>
            <possible id="1">Frank breech</possible>      
            <possible id="2">Complete breech</possible>                    
            <possible id="3">Incomplete breech</possible>                      
            <possible id="4">Acromion presentation</possible>
            <possible id="5">ø</possible>              
            <possible id="0" lang="fr">céphalique (sommet)</possible>
            <possible id="1" lang="fr">siège décomplété</possible>      
            <possible id="2" lang="fr">siège complet</possible>                      
            <possible id="3" lang="fr">siège incomplet</possible>                      
            <possible id="4" lang="fr">transverse (épaule)</possible>   
            <possible id="5" lang="fr">ø</possible>              
            </value>
            <options>resizeToContent;DontPrintEmptyValue</options>          
          </Item>

                                 
      </Item>

